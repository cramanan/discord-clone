{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "http:default",
    "fs:default",
    {
      "identifier": "http:default",
      "allow": [
        "http://127.0.0.1:8080/*"
      ]
    }
  ]
}